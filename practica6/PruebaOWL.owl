    <j.1:title>efectos de una intervencion de ejercicio fisico con videojuegos activos en la composicion corporal de ninos con sobrepeso y obesidad</j.1:title>
    <j.1:creador>
      <j.1:persona rdf:about="http://urldic/creador/domingodelval,daniel">
        <j.1:nombre>domingo del val, daniel</j.1:nombre>
