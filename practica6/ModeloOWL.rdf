<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Class rdf:about="http://mydic/trabajo">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://mydic/documento"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://mydic/documento">
    <rdfs:comment>Archivo de texto del trabajo publicado</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://mydic/entidad">
    <rdfs:comment>Nombre de universidad, departamento u organizacion</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://mydic/persona">
    <rdfs:comment>Nombre de una persona</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://mydic/autor">
    <rdfs:range rdf:resource="http://mydic/persona"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Autor del documento</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mydic/director">
    <rdfs:range rdf:resource="http://mydic/persona"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Persona que dirige el proyecto</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mydic/tema">
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Temas principales del documento</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://mydic/departamento">
    <rdfs:range rdf:resource="http://mydic/entidad"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Organizacion a la que se atribuye la publicacion del documento</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/titulo">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Titulo del documento</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/descripcion">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Resumen del documento</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/fecha">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Fecha de publicacion del documento</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/nombre-persona">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://mydic/persona"/>
    <rdfs:comment xml:lang="es">Nombre completo de la persona</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/identificador">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://mydic/documento"/>
    <rdfs:comment xml:lang="es">Identificador del documento</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://mydic/nombre-departamento">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://mydic/entidad"/>
    <rdfs:comment xml:lang="es">Nombre de la entidad</rdfs:comment>
  </owl:DatatypeProperty>
</rdf:RDF>
